{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}"
			],
			"stopOnEntry": false,
			"sourceMaps": true,
			"outDir": "out",
			"preLaunchTask": "npm"
		}
	],"tasks": [
    {
        "taskName": "less_build",
        "isBuildCommand": false,
        "isTestCommand": false,
        "showOutput": "always",
        "args": []
    },
    {
        "taskName": "less_watch",
        "isBuildCommand": false,
        "isWatching": true,
        "isTestCommand": false,
        "showOutput": "always",
        "args": []
    }
    ]
}